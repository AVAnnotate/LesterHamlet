      "end_time": 1372,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Clip sobre el trauma de perder a alguien que intentó llegar a Estados Unidos en un barco casero."
            }
          ]
        }
      ],
